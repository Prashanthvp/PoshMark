package main;

import model.ServerRequestModel;

public class ServerMain {

	public static void main(String[] args) throws Exception {

		/** Input 1 : Hours
		 * 
		 *  Input 2 : CPUs
		 * 
		 *  Input 3 : Price
		 */		 

		ServerRequestModel s1 = new ServerRequestModel(8,129,0);

		ServerRequestModel s2 = new ServerRequestModel(8,0,77);

		ServerRequestModel s3 = new ServerRequestModel(7,130,77);

		s1.getCosts();

		s2.getCosts();

		s3.getCosts();

	}
}
